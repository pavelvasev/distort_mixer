import components.appmix
import "."
import "https://github.com/pavelvasev/show_ogurca/tree/master/1-structur"

Scene {
  id: scen
  shader: di.shader

  Text {
    text: "Выбор сцены"
    property var tag: "left"
  }

  AppChooser {
    id: chooser
    ctag: "left"
    apps: ["DemoScene1","DemoScene2","Ogurec"]
    //selectedNumber: 1

    Binding {
      target: chooser.app
      property: "shader"
      value: di.shader
    }

  }
  
  DistortMixer {
    id: di
  }
}